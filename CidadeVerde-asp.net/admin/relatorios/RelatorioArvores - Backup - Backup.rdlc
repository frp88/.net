      <Query>
        <DataSourceName>arvoresConnectionString</DataSourceName>
        <CommandText>SELECT     a.codArvore, a.codEspecie, a.dtLevantamento, a.nAltura, a.nDiametroCopa, a.sImagem, a.sCodIdentificacao, a.dtCadastro, a.nStatus, a.dtPlantio, 
                      e.sNomeCientifico
FROM         tblArvore AS a INNER JOIN
                      tblEspecie AS e ON a.codEspecie = e.codEspecie
ORDER BY a.dtCadastro DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
